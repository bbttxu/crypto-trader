{
  "name": "crypto-trader",
  "mod-project-name": "crypto-trader",
  "version": "1.0.0",
  "description": "automated crypto currency trading bot",
  "main": "index.coffee",
  "engines": {
    "node": "8.11.x"
  },
  "scripts": {
    "setup": "yarn install --pure-lockfile",
    "start": "coffee index.coffee",
    "test": "mocha test/*.coffee --exit && istanbul report text-summary lcov",
    "dev": "mocha test/*.{coffee,js} -w",
    "single": "coffee single.coffee",
    "trade": "pm2 start multi.config.js",
    "stop": "pm2 stop multi.config.js",
    "delete": "pm2 delete multi.config.js",
    "reload": "pm2 reload multi.config.js",
    "status": "pm2 status",
    "monit": "pm2 monit",
    "logs": "pm2 logs",
    "chamber": "coffee chamber.coffee",
    "train": "coffee train.coffee",
    "worker": "pm2 start worker.config.js",
    "shift": "pm2 stop worker.config.js",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "build": "babel src --source-maps --out-dir lib",
    "build:watch": "babel src --watch --source-maps --out-dir lib"
  },
  "author": "adam krawiec",
  "license": "ISC",
  "dependencies": {
    "approximate-number": "^2.0.0",
    "axios": "0.17.1",
    "basic-auth-connect": "^1.0.0",
    "blueimp-md5": "^2.10.0",
    "bottleneck": "^2.3.1",
    "coffee-coverage": "3.0.0",
    "coffeescript": "2.1.1",
    "coveralls": "3.0.0",
    "dotenv": "4.0.0",
    "express": "^4.16.3",
    "forever": "0.15.3",
    "gdax": "0.4.2",
    "ioredis": "^3.2.2",
    "istanbul": "0.4.5",
    "knuth-shuffle": "1.0",
    "kue": "0.11.6",
    "left-pad": "^1.2.0",
    "lodash": "^4.17.10",
    "lodash.memoize": "4.1.2",
    "lodash.throttle": "^4.1.1",
    "minimist": "1.2.0",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "moment": "2.19.3",
    "mongodb": "2.2.33",
    "mongoose-find-or-create": "^1.3.1",
    "mongoose-findorcreate": "^3.0.0",
    "neataptic": "^1.4.7",
    "pm2": "2.8.0",
    "ramda": "0.25.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "regression": "2.0.0",
    "rsvp": "^4.8.2",
    "sanctuary": "^0.14.1",
    "should": "13.1.3",
    "summary-statistics": "^1.0.0",
    "synaptic": "^1.1.4",
    "thunk": "0.0.1",
    "treeify": "1.0.1",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "3.0.1"
  }
}
