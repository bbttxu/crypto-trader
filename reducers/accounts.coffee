initialState = {}

accountsReducer = ( state, action )->
  if typeof state == 'undefined'
    return initialState

  if 'UPDATE_ACCOUNTS' is action.type
    state.accounts = action.accounts

    state._hash = md5 JSON.stringify state.accounts

  state


module.exports = accountsReducer

